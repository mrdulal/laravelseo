  "license": "MIT",
  "devDependencies": {
    "laravel-mix": "^6.0.49",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.32.11",
    "sass-loader": "^12.0.0"
  },
  "dependencies": {
    "alpinejs": "^3.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laravel-seo-pro/seo-pro.git"
  },
  "bugs": {
