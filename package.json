{
  "name": "laravel-seo-pro",
  "version": "1.0.0",
  "description": "A comprehensive SEO toolkit for Laravel applications",
  "main": "index.js",
  "scripts": {
    "build": "npm run production",
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "production": "mix --production",
